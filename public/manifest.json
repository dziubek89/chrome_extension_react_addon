{
  "manifest_version": 3,
  "name": "React Content Script Extension",
  "version": "1.0.0",
  "permissions": ["storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ]

}